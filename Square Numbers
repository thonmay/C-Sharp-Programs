namespace Sum_Of_Divisor
{
    internal class Program
    {
        static void Main(string[] args)
        {


            Console.Write("Enter number: ");
            int n = int.Parse(Console.ReadLine());

            for ( int i = 1;  i < n; i++ )
            {

                Console.WriteLine(i * i);
            }

            


        }
    }
}
